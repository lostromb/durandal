{
  "Id": "http://schema.org/MusicRecording",
  "Comment": "A music recording (track), usually a single song.",
  "Label": "MusicRecording",
  "InheritsFrom": [
    "http://schema.org/CreativeWork"
  ],
  "Fields": {
    "recordingOf": {
      "Name": "recordingOf",
      "Values": [
        {
          "Type": "Identifier",
          "ReferencedId": "http://schema.org/MusicComposition",
          "Comment": "The composition this track is a recording of."
        }
      ]
    },
    "isrcCode": {
      "Name": "isrcCode",
      "Values": [
        {
          "Type": "Text",
          "Comment": "The International Standard Recording Code for the recording."
        }
      ]
    },
    "inAlbum": {
      "Name": "inAlbum",
      "Values": [
        {
          "Type": "Identifier",
          "ReferencedId": "http://schema.org/MusicAlbum",
          "Comment": "The album to which this recording belongs."
        }
      ]
    },
    "inPlaylist": {
      "Name": "inPlaylist",
      "Values": [
        {
          "Type": "Identifier",
          "ReferencedId": "http://schema.org/MusicPlaylist",
          "Comment": "The playlist to which this recording belongs."
        }
      ]
    },
    "byArtist": {
      "Name": "byArtist",
      "Values": [
        {
          "Type": "Identifier",
          "ReferencedId": "http://schema.org/MusicGroup",
          "Comment": "The artist that performed this album or recording."
        }
      ]
    },
    "duration": {
      "Name": "duration",
      "Values": [
        {
          "Type": "Identifier",
          "ReferencedId": "http://schema.org/Duration",
          "Comment": "The duration of the item (movie, audio recording, event, etc.) in <a href=\"http://en.wikipedia.org/wiki/ISO_8601\">ISO 8601 date format</a>."
        }
      ]
    }
  }
}